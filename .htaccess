DirectoryIndex index.php
RewriteEngine On

RewriteRule ^404$ 404.php
RewriteRule ^trang-chu$ index.php
RewriteRule ^lien-he$ lienhe.php
RewriteRule ^gioi-thieu$ gioithieu.php
RewriteRule ^([a-zA-Z0-9_-]*)-([0-9]+)/page-([0-9]+)$ danhmuc.php?id=$2&page=$3
RewriteRule ^([a-zA-Z0-9_-]*)/([a-zA-Z0-9,_-]*)-([0-9]+)$ chitiet.php?id=$3
RewriteRule ^([a-zA-Z0-9_-]*)-([0-9]+)$ danhmuc.php?id=$2

